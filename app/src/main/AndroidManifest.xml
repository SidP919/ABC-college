<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.abccollege">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/department_label">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddDeptActivity"
            android:label="@string/add_department_label" />
        <!--<activity android:name=".DeptDetailsActivity" />-->
        <activity
            android:name=".TeachersActivity"
            android:label="@string/teachers_label" />
        <activity
            android:name=".AddTeacherActivity"
            android:label="@string/add_delete_label" />
    </application>

</manifest>